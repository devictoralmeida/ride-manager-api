{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .ts --fix",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "ts-node-dev --rs -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "typeorm": "typeorm-ts-node-commonjs tsconfig-paths/register",
    "typeorm:create": "typeorm-ts-node-commonjs migration:create ./src/shared/infra/typeorm/migrations/",
    "typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/data-source",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source",
    "test": "jest --verbose --runInBand",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "csv-parse": "5.5.0",
    "dayjs": "1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.1",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.1.6",
    "swagger-ui-express": "5.0.0",
    "tsyringe": "4.8.0",
    "typeorm": "0.3.17",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "2.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/jsonwebtoken": "9.0.2",
    "@types/multer": "1.4.7",
    "@types/node": "^20.5.6",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "9.0.2",
    "eslint": "^8.47.0",
    "jest": "29.7.0",
    "sqlite": "5.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  }
}
